<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.taskmanager.controllers.ExpandirActividadViewController"
            prefHeight="271.0" prefWidth="354.0">
    <children>
        <Label layoutX="99.0" layoutY="1.0" text="Expansión de Actividad">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
        <Hyperlink layoutY="3.0" onAction="#onSalirClick" text="Salir" underline="true" />
        <Label layoutX="145.0" layoutY="35.0" text="Descripción">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label fx:id="lblDescripcion" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="59.0" prefHeight="18.0" prefWidth="328.0" text="Label" textAlignment="CENTER" />
        <Label layoutX="136.0" layoutY="88.0" text="¿Es obligatoria?">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label fx:id="lblObligatoria" alignment="CENTER" contentDisplay="CENTER" layoutX="16.0" layoutY="106.0" prefHeight="18.0" prefWidth="328.0" text="Label" textAlignment="CENTER" />
       <Label layoutX="138.0" layoutY="187.0" text="¿Completado?">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label fx:id="lblCompletado" alignment="CENTER" contentDisplay="CENTER" layoutX="16.0" layoutY="205.0" prefHeight="18.0" prefWidth="328.0" text="Label" textAlignment="CENTER" />
        <Button layoutX="144.0" layoutY="237.0" mnemonicParsing="false" onAction="#onCompletar" text="Completar" />
    </children>

</AnchorPane>
