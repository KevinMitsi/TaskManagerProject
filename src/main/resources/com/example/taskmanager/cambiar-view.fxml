<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="236.0" prefWidth="436.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.taskmanager.controllers.CambiarViewController">
    <Label layoutX="130.0" layoutY="13.0" text="Cambiar Actividades">
        <font>
          <Font name="System Bold" size="17.0"/>
        </font>
    </Label>
    <Hyperlink layoutY="2.0" onAction="#onSalir" text="Salir" underline="true"/>
    <ComboBox fx:id="cbActividad1" layoutX="137.0" layoutY="92.0" prefWidth="150.0" promptText="seleccione"/>
    <Label layoutX="6.0" layoutY="46.0"
           text="Para intercambiar una actividad debe elijir las dos actividades que van a cambiar"/>
    <Label layoutX="184.0" layoutY="74.0" text="Actividad 1:">
        <font>
          <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <ComboBox fx:id="cbActividad2" layoutX="137.0" layoutY="159.0" prefWidth="150.0" promptText="seleccione"/>
    <Label layoutX="184.0" layoutY="141.0" text="Actividad 2:">
        <font>
          <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <Button layoutX="187.0" layoutY="209.0" mnemonicParsing="false" onAction="#onCambiar" text="Cambiar"/>
</AnchorPane>
