<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="135.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.taskmanager.controllers.SearchTaskViewController">
    <ComboBox fx:id="cbOpciones" disable="true" layoutX="100.0" layoutY="95.0" onAction="#onClickOpciones" prefWidth="150.0" promptText="elija el empleado" />
    <Hyperlink layoutY="3.0" onAction="#onVolverClick" text="Volver" underline="true" />
    <Button layoutX="250.0" layoutY="55.0" mnemonicParsing="false" onAction="#onBuscar" text="Buscar" />
    <Label layoutX="124.0" layoutY="14.0" text="Buscar Tarea">
        <font>
            <Font name="System Bold" size="17.0" />
        </font>
    </Label>
   <TextField fx:id="tfKeyWord" layoutX="64.0" layoutY="55.0" promptText="Escriba una palabra clave" />

</AnchorPane>
