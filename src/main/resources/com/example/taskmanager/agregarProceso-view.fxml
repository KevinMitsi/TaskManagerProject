<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.taskmanager.controllers.AgregarProcesoViewController"
            prefHeight="135.0" prefWidth="460.0">
    <children>
        <ComboBox fx:id="cbOpciones" layoutX="155.0" layoutY="55.0" prefWidth="150.0" promptText="elija el empleado" />
        <Hyperlink layoutY="3.0" onAction="#onVolverClick" text="Volver" underline="true" />
        <Button layoutX="200.0" layoutY="108.0" mnemonicParsing="false" onAction="#onAgregar" text="Agregar" />
        <Label layoutX="169.0" layoutY="14.0" text="Crear Proceso">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
