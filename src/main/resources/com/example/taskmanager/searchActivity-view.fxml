<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.taskmanager.controllers.SearchActivityViewController"
            prefHeight="135.0" prefWidth="350.0">
    <ComboBox fx:id="cbOpciones" disable="true" layoutX="100.0" layoutY="95.0" onAction="#onClickOpciones" prefWidth="150.0" promptText="elija el empleado" />
    <Hyperlink layoutY="3.0" onAction="#onVolverClick" text="Volver" underline="true" />
    <Button layoutX="250.0" layoutY="55.0" mnemonicParsing="false" onAction="#onBuscar" text="Buscar" />
    <Label layoutX="124.0" layoutY="14.0" text="Buscar Tarea">
        <font>
            <Font name="System Bold" size="17.0" />
        </font>
    </Label>
    <TextField fx:id="tfKeyWord" layoutX="64.0" layoutY="55.0" promptText="Escriba una palabra clave" />


</AnchorPane>
